{
  "name": "large-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*/*"
  ],
  "scripts": {
    "postinstall": "nx daemon",
    "benchmark": "node benchmark.js"
  },
  "devDependencies": {
    "prettier": "^2.3.1",
    "@nrwl/workspace": "13.4.4-beta.4",
    "@nrwl/cli": "13.4.4-beta.4",
    "@nrwl/tao": "13.4.4-beta.4",
    "turbo": "1.0.11",
    "typescript": "4.5.2",
    "lage": "^1.3.0"
  },
  "turbo": {
    "pipeline": {
      "build": {},
      "crew#build": {
        "outputs": [".next/**"]
      },
      "flight-simulator#build": {
        "outputs": [".next/**"]
      },
      "navigation#build": {
        "outputs": [".next/**"]
      },
      "ticket-booking#build": {
        "outputs": [".next/**"]
      },
      "warp-drive-manager#build": {
        "outputs": [".next/**"]
      }
    }
  }
}
